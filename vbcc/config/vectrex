-cc=vbcchc12 -cpu=6809 -I"%%VBCC%%"/targets/6809-vec/include -quiet %s -o= %s %s -O=%ld
-ccv=vbcchc12 -cpu=6809 -I"%%VBCC%%"/targets/6809-vec/include %s -o= %s %s -O=%ld
-as=vasm6809_std -quiet -nowarn=62 -opt-branch -opt-offset -Fvobj %s -o %s 
-asv=vasm6809_std -nowarn=62 -Fvobj -opt-branch -opt-offset -v %s -o %s
-rm=del %s
-rmv=del %s
-ld=vlink -b rawbin1 -Cvbccelf -T"%%VBCC%%"/targets/6809-vec/vlink.cmd -L"%%VBCC%%"/targets/6809-vec/lib "%%VBCC%%"/targets/6809-vec/lib/startup.o %s %s -o %s -lvc -Mbla
-ldv=vlink -v  -b rawbin1 -Cvbccelf -T"%%VBCC%%"/targets/6809-vec/vlink.cmd -L"%%VBCC%%"/targets/6809-vec/lib "%%VBCC%%"/targets/6809-vec/lib/startup.o %s %s  -o %s -lvc -Mbla
